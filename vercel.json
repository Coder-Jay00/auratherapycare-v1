{
  "version": 2,
  "functions": {
    "server.js": {
      "includeFiles": [
        "*.html",
        "css/**",
        "js/**",
        "*.ico",
        "*.png",
        "*.jpg",
        "*.jpeg",
        "*.svg",
        "*.woff",
        "*.woff2",
        "*.ttf",
        "*.eot"
      ]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
